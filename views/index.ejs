<%- include ('./partials/header.ejs') %> 
<body>
    <%- include ('./partials/homenavbar.ejs') %> 
    <div class="container">
        <hr>
        <div class="jumbotron">
            <h1 class="display-4">Wellcome</h1>
            <p class="lead">This is a simple Blog for praticing</p>
            <hr class="my-4">
            <p>text complex</p>
            <p class="lead">
              <a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a>
            </p>
          </div>
        <hr>
        <% articles.forEach(article => { %>
            <div class="card">
                <div class="card-header">
                    <h2><%= article.title %> </h2>
                </div>
                <div class="card-body">
                    <a class="btn btn-success" href="/<%= article.slug %> ">Read Article</a>
                </div>
            </div>
            <br>
        <% }) %>
        <div class="row">
            <div class="col col-12">
                <a href="/articles/page/2" style="float: right;">Next</a>
            </div>
            <br>
        </div>
        <br>
    </div>
</body>
